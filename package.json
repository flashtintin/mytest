{
  "name": "{{name}}",
  "version": "1.0.0",
  "description": "{{description}}",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-dist": "webpack --config build/webpack.prod.config",
    "prebuild": "yarn || npm i && shx rm -rf pkg && shx rm -rf build_temp && shx mkdir build_temp",
    "build": "webpack --config build/webpack.prod.config",
    "postbuild": "shx mv build/dist build_temp && npm run cp-files && cd build_temp && yarn install --production && npm run compressing",
    "cp-files": "shx cp -r {config,middleware,modules,index.js,favicon.ico,package.json,pm2.json,Dockerfile,.dockerignore} build_temp",
    "compressing": "cd .. && node compress",
    "predeploy": "yarn || npm i && shx rm -rf build_temp && shx mkdir build_temp",
    "deploy": "yarn build-dist && shx mv build/dist build_temp && npm run cp-files && cd build_temp && yarn install --production && yarn clean-container && yarn clean-images && yarn docker-start",
    "docker-start": "docker build -t $npm_package_name:$npm_package_version --no-cache . && docker run -d -p 3000:3000 $npm_package_name:$npm_package_version",
    "precommit": "lint-staged",
    "install-temp": "shx mv build/dist build_temp && yarn cp-files && cd build_temp && yarn install --production",
    "clean-container": "docker rm -f $(docker ps -a | grep \"$npm_package_name\" | awk \"{print \\$1}\") || true",
    "clean-images": "docker rmi -f $(docker images | grep \"$npm_package_name\" | awk \"{print \\$3}\") || true",
    "build-temp": "docker build -t $npm_package_name:$npm_package_version --no-cache .",
    "tagimage": "docker tag $npm_package_name:$npm_package_version registry.docker.fe.ioteams.com/gateway/$npm_package_name:$npm_package_version",
    "preregistry": "cnpm i && shx rm -rf build_temp && shx mkdir build_temp && yarn build-dist && yarn install-temp",
    "registry": "yarn clean-container && yarn clean-images && yarn build-temp",
    "postregistry": "yarn tagimage && docker push registry.docker.fe.ioteams.com/gateway/$npm_package_name:$npm_package_version"
  },
  "keywords": [
    "vue",
    "cli"
  ],
  "author": "{{author}}",
  "license": "ISC",
  "dependencies": {
    "{{proxy}}": "{{proxyVersion}}",
    "connect-history-api-fallback": "^1.5.0",
    "ioredis": "^4.14.0",
    "koa": "^2.4.1",
    "koa-body": "2.0.1",
    "koa-favicon": "^2.0.0",
    "koa-router": "^7.4.0",
    "koa-session2": "^2.2.10",
    "koa-static-cache": "^5.1.1",
    "request": "^2.83.0",
    "uuid": "^3.3.2",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^9.1.5",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.6",
    "babel-helper-vue-jsx-merge-props": "^2.0.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-component": "^0.10.1",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-transform-vue-jsx": "^3.5.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "clean-webpack-plugin": "^0.1.19",
    "compressing": "^1.3.1",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "eslint": "4.19.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-html": "^4.0.5",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "koa-webpack": "3.0.0",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "lint-staged": "^7.3.0",
    "mini-css-extract-plugin": "^0.4.2",
    "mkdirp": "^0.5.1",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss": "^7.0.2",
    "postcss-loader": "^3.0.0",
    "postcss-pxtorem": "^4.0.1",
    "rimraf": "2.6.1",
    "shx": "^0.3.2",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "url-loader": "^1.1.1",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^15.7.1",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.5.17",
